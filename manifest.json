{
  "manifest_version": 2,
  "name": "Website Blocker",
  "version": "1.0.0",
  "description": "A Test Project to Block Websites",
  "short_name": "website_blocker",
  "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["background.css"],
      "js": ["background.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
}
